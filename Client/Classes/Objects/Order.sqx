namespace Intrusion.Client
{
	public class Order
	{
		// Creates a Order object.
		public constructor("_group" as Group, "_type" as OrderType, "_targetPosition" as Array)
		{
			_self.Group = _group;
			_self.Type = _type;
			_self.TargetPosition = _targetPosition;
		};
		
		public property Group Group { get; private set; };
		
		public property OrderType Type { get; private set; };
		
		public property TargetPosition { get; private set; };
	};
};

