 */
namespace Intrusion.Ai
{
	public class EngageAction : Action
	{
		// Creates a EngageAction object.
		public constructor("_timeoutTime" as Scalar, "_position" as Array)
		{
			[_timeoutTime] call _base.Constructor;
			_self.Position = _position;
		};
		
		// Gets the position that are about to be engaged.
		public property Array Position { get; private set; };
	};
};
