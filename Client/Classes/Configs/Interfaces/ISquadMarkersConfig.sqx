 *
 * Description:
 * Contains config information about map markers concerning squads.
 */
namespace Intrusion.Client
{
	public interface ISquadMarkersConfig
	{
		// Gets the marker type of the squad's vehicle on the map.
		property String SquadVehicleMarkerType { get; };
		
		// Gets the marker type of the squad's member on the map.
		property String SquadMemberMarkerType { get; };
		
		// Gets the marker color of the player's marker on the map.
		property String PlayerMarkerColor { get; };
	};
};
