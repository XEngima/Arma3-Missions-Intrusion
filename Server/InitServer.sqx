// Make resistance enemy to all both east and west.

resistance setFriend [west, 0];
west setFriend [resistance, 0];
resistance setFriend [east, 0];
east setFriend [resistance, 0];

#region Server Classes

/* Metas */
call WeaponBoxTypeMeta.Init;

/* Configs */
call WeaponsConfig.Init;
call WeaponBoxesConfig.Init;
call ServerEventHandlers.Init;

gServerConfig = [ReleaseSettings.Mode] call ServerConfigFactory.CreateServerConfig as IServerConfig;
gServer = [new AiUnitCounter] new IntrusionServer;

/* Handlers */
[gCommonConfig.MapMarkers, gCommon.MissionParametersHandler.AiSkillLevel] call ResourceLocationHandler.Init;

/* Services */
[gServer.TargetHandler] call ObservationService.Init;

/* AI */

[] spawn {
	private ["_aiService" as IService, "_aiUnitBalancer" as IAiUnitBalancer];
	
	_aiUnitBalancer = [gServer.UnitCounter, gCommon.MissionParametersHandler.AiWestFrequency, gCommon.MissionParametersHandler.AiEastFrequency] new AiUnitBalancer;
	_aiService = [_aiUnitBalancer, gServerConfig.Gameplay, new AiUnitsConfig, gCommonConfig.MapMarkers, gServer.VehicleSpawner, gCommonConfig.Professions, gServer.SquadHandler, gCommon.MissionParametersHandler.AiSkillLevel] new AiService;
	sleep 5;
	call _aiService.RunAsync;
};

