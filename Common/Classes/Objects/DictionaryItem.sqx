namespace Intrusion.Common
{
	public class DictionaryItem
	{
		// Creates a DictionaryItem object.
		public constructor("_key" as Any, "_value" as Any)
		{
			_self.Key = _key;
			_self.Value = _value;
		};
		
		public property Any Key { get; private set; };
		
		public property Any Value { get; private set; };
	};
};
