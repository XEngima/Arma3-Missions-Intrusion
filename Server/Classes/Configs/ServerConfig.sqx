namespace Intrusion.Server
{
	public class ServerConfig : IServerConfig
	{
		// Creates a ServerConfig object.
		public constructor("_gameplayConfig" as IGameplayConfig, "_technologyConfig" as ITechnologyConfig) {
			_self.Gameplay = _gameplayConfig;
			_self.Technology = _technologyConfig;
		};
		
		public property IGameplayConfig Gameplay { get; private set; };
		
		public property ITechnologyConfig Technology { get; private set; };
	};
};
