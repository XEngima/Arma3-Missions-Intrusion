namespace Intrusion.Common
{
	public class VehicleCargoInfo
	{
		// Creates a VehicleCargoInfo object.
		public constructor(
			"_className" as String,
			["_quantity" as Scalar, 1]
		) {
			_self.ClassName = _className;
			_self.Quantity = _quantity;

		};
		
		// Objects to add to the vehicle cargo, 
		// or attachment if it's set.
		public property ClassName { get; private set; };
		
		// How many objects to add.
		public property Quantity { get; private set; };

		
	};
};

